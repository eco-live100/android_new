                    android:layout_width="@dimen/_38sdp"
                    android:layout_height="@dimen/_38sdp"
                    android:background="@drawable/circle_blue120"
                    android:fontFamily="@font/roboto_regular"
                    android:gravity="center"
                    android:text="GA"
                    android:backgroundTint="@color/grey"
                    android:textColor="@color/transparent"
                    android:textSize="@dimen/_14sdp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/userNameTxt"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"

                    android:text="Gerry Atric"
                    android:layout_marginStart="@dimen/_7sdp"
                    android:textColor="@color/transparent"
                    android:textSize="@dimen/sp_16"
                    android:background="@color/grey"
                    android:fontFamily="@font/roboto_medium"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/nameLogo"
                    app:layout_constraintTop_toTopOf="@+id/nameLogo" />

                <TextView
                    android:id="@+id/userPhoneTxt"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/roboto_regular"
                    android:text="01234 56789"
                    android:layout_marginStart="@dimen/_7sdp"
                    android:textColor="@color/transparent"
                    android:textSize="@dimen/sp_12"
                    android:background="@color/grey"
                    android:layout_marginTop="@dimen/_5sdp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/nameLogo"
                    app:layout_constraintTop_toBottomOf="@+id/userNameTxt" />



